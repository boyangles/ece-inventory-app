<p>
  <b>Bulk Import</b>
</p>

<%= form_tag({action: :bulk_import}, multipart: true) do |f| %>
    <% if @imported_error_msg %>
        <div id="error_explanation">
          <pre>
            <%= @imported_error_msg %>
          </pre>
        </div>
    <% end %>

    <%= file_field_tag 'items_as_json' %>
    <%= submit_tag "Upload JSON file", class: 'btn btn-primary' %>
<% end %>

<%= link_to 'Back', :back %>
