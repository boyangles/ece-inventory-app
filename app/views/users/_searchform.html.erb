<div class="search">
  <%= form_for items_path, :method => 'get' do |f| %>



      <h5 class="pull-right col-lg-2">
        <%= link_to "Search", '#', id: 'search-link' %></br>
      </h5>
      <div class="advanced-search">
        <div class="form-group">
          <%= f.label "Search Users", :class=>"col-lg-3 control-label" %><br>
          <div class="col-lg-12">
            <%= text_field_tag :search, params[:search], :placeholder => "Item", :class => "form-control" %>
          </div>
        </div>
        <%= f.submit "Search", class: "btn btn-default col-lg-2 pull-left", type: "submit" %> </br> </br>

      </div>


  <% end %>

</div>
